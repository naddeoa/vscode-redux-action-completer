{
  "name": "actionfinder",
  "displayName": "vscode-scroll-percentage-plugin",
  "description": "Shows the percent position in the page",
  "version": "0.0.1",
  "publisher": "whatever",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:naddeo.hello",
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "naddeo.hello",
        "title": "Naddeo"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Redux action finder",
      "properties": {
        "modules": {
          "type": "array",
          "default": [
            "uuid",
            "typescript"
          ],
          "description": "Modules to search for actions"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@types/lodash": "^4.14.64",
    "acorn": "^4.0.13",
    "lodash": "^4.17.4"
  }
}
