{
  "name": "redux-action-completer",
  "displayName": "Redux action completer",
  "description": "Autocomplete actions when dispatching",
  "version": "0.0.1",
  "publisher": "naddeo",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "naddeo.hello",
        "title": "Naddeo"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Redux action completer",
      "properties": {
        "redux-action-finder.modules": {
          "type": "array",
          "default": [],
          "description": "Modules to search for actions"
        },
        "redux-aciton-finder.fileGlobs": {
          "type": "array",
          "default": [],
          "description": "Glob patterns to match file names that contain actions"
        },
        "redux-aciton-finder.nodeModulePaths": {
          "type": "array",
          "default": ["node_modules"],
          "description": "The paths of node module folders. Pretty much the same as the NODE_MODULES env variable."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@types/acorn": "^4.0.2",
    "@types/lodash": "^4.14.64",
    "acorn": "^4.0.13",
    "lodash": "^4.17.4"
  }
}
